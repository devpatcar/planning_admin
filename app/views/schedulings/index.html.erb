<p id="notice"><%= notice %></p>

<h1>Schedulings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Template</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Monday</th>
      <th>Thuesday</th>
      <th>Wendsday</th>
      <th>Thursday</th>
      <th>Friday</th>
      <th>Saturday</th>
      <th>Sunday</th>
      <th>Week duration</th>
      <th>Delay</th>
      <th>Notification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedulings.each do |scheduling| %>
      <tr>
        <td><%= scheduling.name %></td>
        <td><%= scheduling.template_id %></td>
        <td><%= scheduling.start_date %></td>
        <td><%= scheduling.end_date %></td>
        <td><%= scheduling.monday %></td>
        <td><%= scheduling.thuesday %></td>
        <td><%= scheduling.wendsday %></td>
        <td><%= scheduling.thursday %></td>
        <td><%= scheduling.friday %></td>
        <td><%= scheduling.saturday %></td>
        <td><%= scheduling.sunday %></td>
        <td><%= scheduling.week_duration %></td>
        <td><%= scheduling.delay %></td>
        <td><%= scheduling.notification %></td>
        <td><%= link_to 'Show', scheduling %></td>
        <td><%= link_to 'Edit', edit_scheduling_path(scheduling) %></td>
        <td><%= link_to 'Destroy', scheduling, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scheduling', new_scheduling_path %>
